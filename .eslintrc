{
  "extends": "eslint-config-airbnb",
  "globals": {
    "chrome": true,
    "__DEVELOPMENT__": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "block-scoped-var": 0,
    "padded-blocks": 0,
    "quotes": [ 1, "single" ],
    "comma-style": [ 2, "last" ],
    "eol-last": [ 0 ],
    "no-unused-vars": [ 0 ],
    "no-console": [ 0 ]
  },
  "plugins": [
    "react"
  ]
}